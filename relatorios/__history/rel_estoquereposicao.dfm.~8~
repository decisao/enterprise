  inherited cdsRelatorio: TClientDataSet
    Active = True
    CommandText = 
      'SELECT PO.codigo, PO.barra, PO.partnumber, PO.descricao, COALESC' +
      'E((SELECT SUM(ID1.QUANTIDADE) FROM INDIVIDUAIS ID1 WHERE ID1.COD' +
      'PRODUTO = PO.CODIGO AND ID1.VENDIDO = '#39'N'#39' AND ID1.codempresa = 1' +
      '), 0) AS estoque, COALESCE((SELECT SUM(ID2.QUANTIDADE) FROM INDI' +
      'VIDUAIS ID2 JOIN MOVIMENTOS MO2 ON (MO2.CODIGO = ID2.codmovsaida' +
      ') WHERE ID2.CODPRODUTO = PO.CODIGO AND ID2.vendido = '#39'S'#39' AND MO2' +
      '.es = 2 AND MO2.tipo = 6 AND ID2.codempresa = 1 AND MO2.datafech' +
      'amento BETWEEN '#39'01/01/2018'#39' AND '#39'01/31/2018'#39'), 0) AS vendidos, P' +
      'O.minimo, PO.precocusto FROM PRODUTOS PO WHERE PO.PS= '#39'P'#39' ORDER ' +
      'BY 6 DESC'
