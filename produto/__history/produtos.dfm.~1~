  inherited cdsDados: TClientDataSet
    CommandText = 
      'SELECT '#39'N'#39' AS SEL, PO.*, (SELECT SUM(INDIVIDUAIS.QUANTIDADE) FRO' +
      'M INDIVIDUAIS WHERE INDIVIDUAIS.CODPRODUTO = PO.CODIGO AND INDIV' +
      'IDUAIS.VENDIDO = '#39'N'#39' AND INDIVIDUAIS.CODEMPRESA = #CODEMPRESA#) ' +
      'AS ESTOQUE, (SELECT SUM(INDIVIDUAIS.QUANTIDADE) FROM INDIVIDUAIS' +
      ' WHERE INDIVIDUAIS.CODPRODUTO = PO.CODIGO AND INDIVIDUAIS.VENDID' +
      'O = '#39'N'#39') AS ESTOQUETOTAL, SE.DESCRICAO AS DESCRICAO_1, MA.DESCRI' +
      'CAO AS MARCA, MO.DESCRICAO AS MODELO, CE.DESCRICAO AS CENTRO FRO' +
      'M PRODUTOS PO LEFT JOIN PRODUTOS SE ON (SE.CODIGO = PO.CODSERVIC' +
      'O) LEFT JOIN MARCAS MA ON (MA.CODIGO = PO.CODMARCA) LEFT JOIN MO' +
      'DELOS MO ON (MO.CODIGO = PO.CODMODELO AND MO.CODEQUIPAMENTO = PO' +
      '.CODMARCA) LEFT JOIN CENTROS CE ON (CE.CODIGO = PO.CODCENTRO) WH' +
      'ERE PO.PS= '#39'P'#39
    ProviderName = 'pvProdutos'
