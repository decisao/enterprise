  inherited cdsDados: TClientDataSet
    CommandText = 
      'SELECT '#39'N'#39' AS SEL, CLIENTES.*, (SELECT SUM(PA.VALOR) FROM PAGAME' +
      'NTOS PA WHERE PA.CODCLIENTE = CLIENTES.CODIGO AND PA.PAGO = 0 AN' +
      'D PA.ES = 2) AS ABERTO FROM CLIENTES'
    ProviderName = 'pvPessoas'
    Left = 544
    Top = 256
